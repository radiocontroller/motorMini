/* pages/map/map.wxss */
page{ background-color: #000; width: 100%; height: 100%;}
#myMap{ width:100%; height: 100vh; }

.customCallout {
  box-sizing: border-box;
  background-color: #fff;
  /* border: 1px solid #ccc; */
  border-radius: 10.86rpx;
  width: 289.6rpx;
  height: 72.4rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}

.content {
  display: flex;
  flex-direction: column;
  width: 217.2rpx;
  font-size: 25.34rpx;
}

.name {
  font-size: 23.53rpx;
  text-align: center;
}

.distance {
  font-size: 18.1rpx;
  text-align: center;
  color: #8E8E8E;
  padding-top: 3.62rpx;
}

.go {
  font-size: 28.96rpx;
  width: 72.4rpx;
  line-height: 72.4rpx;
  background-color:	#FF7F00;
  text-align: center;
  color: #404040;
}

.toggle-callout {
  width: 181rpx;
  height: 54.3rpx;
  position: absolute;
  bottom: 36.2rpx;
  left: 18.1rpx;
  font-size: 10px;
  border: none;
}